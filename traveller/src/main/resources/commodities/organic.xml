<?xml version="1.0" encoding="UTF-8"?>

<commodities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xmlns:w="http://glendale.org.uk/mapcraft/worldgen"
			 xsi:schemaLocation="commodities.xsd">

    <group name="Base">
        <commodity name="Food">
            <cost volume="1">50</cost>
            <production tech="0" law="6" pr="4" cr="5"/>
            <codes source="Ag">Fo Pe</codes>
        </commodity>
    </group>
    
	<group name="Base Organics" base="organics">
	    <commodity name="Protobionts" parent="Food">
	        <cost volume="1">7</cost>
            <production tech="8" law="6" pr="5" cr="6"/>
	        <codes source="Ag">Fo FoOr Lq Pe</codes>
	        <description>
	            Abiotically produced organic molecules. May have a simple
	            cell-like membrane structure, and be capable of simple
	            self-replication.
	        </description>
	    </commodity>

	    <commodity name="Prokaryotes" parent="Food">
	    	<cost volume="1">5</cost>
	    	<production tech="8" law="6" pr="5" cr="6"/>
	    	<codes source="Ag">Fo FoOr Lq Pe</codes>
	    	<description>
	    		Simple organisms which lack a cell nucleus, though may have a
	    		simple cell structure. They are the simplest living things, 
	    		evolved from protobionts.
	    	</description>
	    </commodity>
    
	    <commodity name="Cyanobacteria" parent="Food">
	    	<source>Ag</source><image>algae</image>
	    	<cost volume="1">9</cost>
	    	<production tech="7" law="6" pr="4" cr="5"/>
	    	<codes source="Ag">Fo FoOr Lq Pe</codes>
	    	<description>
	    		A phylum of bacteria also known as blue-green algae that
	    		uses photosynthesis to generate oxygen. They have simple
	    		cell structures and are often precursors to true algae.
	    	</description>
	    </commodity>

	    <commodity name="Algae" parent="Food">
	        <cost volume="1">12</cost>
	        <production tech="6" pr="5" cr="6"/>
	        <codes source="Ag">Fo FoOr Lq Pe</codes>
	        <description>
	            Multicellular eukaryotic life, ranging from simple clusters of
	            cells to huge kelp forests. Found in the seas of most life
	            bearing worlds. Also known as seaweed.
	        </description>
	    </commodity>
    
	    <commodity name="Cnidarians" parent="Food">
	    	<cost volume="1">25</cost>
	    	<production tech="1" law="6" pr="4" cr="5"/>
	    	<codes source="Ag">Fo FoFi Lq Pe</codes>
	    	<description>
	    		Simple sea creatures such as jellies, sea ameneonies and hydras.
	    	</description>
	    </commodity>
    
	    <commodity name="Echinoderms" parent="Food">
	    	<cost volume="1">50</cost>
	    	<production tech="1" law="6" pr="6" cr="7"/>
	    	<codes source="Ag">Fo FoFi Pe</codes>
	    	<description>
	    		Bony sea creatures such as starfish.
	    	</description>
	    </commodity>

	    <commodity name="Marine Arthropods" parent="Food">
	    	<cost volume="1">75</cost>
	    	<production tech="1" law="6" pr="7" cr="8"/>
	    	<codes source="Ag">Fo FoFi Pe</codes>
	    	<description>
	    		Sea dwelling crustaceans.
	    	</description>
	    </commodity>

        <commodity name="Fish" parent="Food">
            <cost volume="1">90</cost>
            <production tech="1" law="6" pr="7" cr="8"/>
            <codes source="Ag">Fo FoFi Pe</codes>
            <description>
                Sea dwelling vertebrates.
            </description>
        </commodity>

        <commodity name="Moss" parent="Food">
            <cost volume="1">10</cost>
            <production tech="6" law="6" pr="5" cr="5"/>
            <codes source="Ag">Fo Pe Po</codes>
            <description>
                Simple land plants.
            </description>
        </commodity>

        <commodity name="Fungi" parent="Food">
            <cost volume="1">30</cost>
            <production tech="0" law="6" pr="5" cr="6"/>
            <codes source="Ag">Fo Pe Po</codes>
            <description>
                Spore based land life.
            </description>
        </commodity>
        
        <commodity name="Ferns" parent="Food">
            <cost volume="1">50</cost>
            <production tech="0" law="6" pr="6" cr="7">
                <output mode="Ag">Vegetables</output>
            </production>
            <codes source="Ag">Pe</codes>
            <description>
                Any form of land planet which has neither flowers now woody
                stems. They are considered more primitive than other plants.
            </description>
        </commodity>

        <commodity name="Trees" parent="Food">
            <cost volume="1">60</cost>
            <production tech="0" law="6" pr="6" cr="7">
                <output mode="Ag">Vegetables</output>
                <output mode="Ag">Fruit</output>
                <output mode="AgFa">Wood</output>
            </production>
            <codes source="Ag">PR</codes>
            <description>
                Any of a broad classification of woody plants.
            </description>
        </commodity>

        <commodity name="Grasses" parent="Food">
            <cost volume="1">60</cost>
            <production tech="0" law="6" pr="6" cr="7">
                <output mode="AgFa">Grain</output>
            </production>
            <codes source="Ag">Pe</codes>
            <description>
                A type of planet.
            </description>
        </commodity>

        <commodity name="Vegetables" parent="Food">
            <cost volume="1">50</cost>
            <production tech="0" law="6" pr="6" cr="7"/>
            <codes source="Ag">Fo Pe</codes>
            <description>
                Edible vegetable matter.
            </description>
        </commodity>

        <commodity name="Fruit" parent="Food">
            <cost volume="1">75</cost>
            <production tech="0" law="6" pr="6" cr="7"/>
            <codes source="Ag">Fo Pe</codes>
            <description>
                Edible seed bearing fruits.
            </description>
        </commodity>

        <commodity name="Grain" parent="Food">
            <cost volume="1">40</cost>
            <production tech="0" law="6" pr="6" cr="7"/>
            <codes source="Ag">Fo Pe</codes>
            <description>
                Edible seed bearing fruits.
            </description>
        </commodity>
        
        <commodity name="Animals">
            <cost volume="1">500</cost>
            <production tech="0" law="6" pr="8" cr="8"/>
            <codes source="Ag">Pe</codes>
            <description>
                Parent holder for all types of land animals.
            </description>
        </commodity>
        
        <commodity name="Mammals" parent="Animals">
            <cost volume="1">500</cost>
            <production tech="0" law="6" pr="10" cr="11"/>
            <codes source="Ag">Pe Fr FoHd FoHu</codes>
        </commodity>

        <commodity name="Meat" parent="Food">
            <cost volume="1">200</cost>
            <production tech="0" law="6" pr="10" cr="11"/>
            <codes source="Ag">Pe Fr AgHd AgHu</codes>
        </commodity>
    </group>
</commodities>